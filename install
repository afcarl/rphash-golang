go install github.com/wenkesj/rphash/parse
echo "Installing types"
go install github.com/wenkesj/rphash/types
echo "Installing utils"
go install github.com/wenkesj/rphash/utils
echo "Installing decoder"
go install github.com/wenkesj/rphash/decoder
echo "Installing murmur"
go install github.com/wenkesj/rphash/hash
echo "Installing itemset"
go install github.com/wenkesj/rphash/itemset
echo "Installing clusterer"
go install github.com/wenkesj/rphash/clusterer
echo "Installing lsh"
go install github.com/wenkesj/rphash/lsh
echo "Installing projection"
go install github.com/wenkesj/rphash/projector
echo "Installing reader"
go install github.com/wenkesj/rphash/reader
echo "Installing defaults"
go install github.com/wenkesj/rphash/defaults
echo "Installing simple"
go install github.com/wenkesj/rphash/simple
echo "Installing stream"
go install github.com/wenkesj/rphash/stream
echo "Installation complete!"
